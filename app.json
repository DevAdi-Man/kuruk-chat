{
  "expo": {
    "name": "kurukChat",
    "slug": "kurukChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kurukchat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devadiman.kurukChat"
    },
    "android": {
      "adaptiveIcon": {
        "googleServicesFile": "./google-services.json",
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.devadiman.kurukChat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
        "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra":{
      "app_key":"AIzaSyA7fz-pFV7dGMECSH7-_lBSo8KZaFQcE0w",
      "auth_domain":"kuruk-chat.firebaseapp.com",
      "project_id":"kuruk-chat",
      "storage_bucket":"kurukchat.appspot.com",
      "app_id":"1:989831167249:android:04baba4dfde8d002c820b0",
      "database_url":"https://kuruk-chat-default-rtdb.firebaseio.com/",
      "webClientId": "989831167249-a52bd6m4noj00l1ecrn2octs0svs8tek.apps.googleusercontent.com"
    }
  }
}
